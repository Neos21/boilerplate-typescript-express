{
  "name": "typescript-express-boilerplate",
  "description": "TypeScript Express Boilerplate",
  "private": true,
  "scripts": {
    "eslint": "eslint",
    "jest": "jest",
    "ts-node": "ts-node",
    "tsc": "tsc",
    "start": "node ./dist/index.js",
    "test": "jest --coverage",
    "start-ts": "ts-node ./src/index.ts",
    "dev": "ts-node-dev ./src/index.ts",
    "lint": "eslint --ext '.js,.ts' './src/**'",
    "lint-fix": "eslint --fix --ext '.js,.ts' './src/**'",
    "build": "tsc"
  },
  "author": "Neo <neos21@gmail.com> (http://neo.s21.xrea.com/)",
  "license": "MIT",
  "homepage": "https://github.com/Neos21/typescript-express-boilerplate#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Neos21/typescript-express-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/Neos21/typescript-express-boilerplate/issues"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/Neos21"
  },
  "dependencies": {
    "body-parser": "1.19.0",
    "express": "4.17.1"
  },
  "devDependencies": {
    "@types/express": "4.17.6",
    "@types/jest": "25.2.3",
    "@types/node": "14.0.5",
    "@types/supertest": "2.0.9",
    "@typescript-eslint/eslint-plugin": "3.0.0",
    "@typescript-eslint/parser": "3.0.0",
    "eslint": "7.1.0",
    "jest": "26.0.1",
    "supertest": "4.0.2",
    "ts-jest": "26.0.0",
    "ts-node": "8.10.1",
    "ts-node-dev": "1.0.0-pre.44",
    "typescript": "3.9.3"
  }
